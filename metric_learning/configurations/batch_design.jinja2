{
    'name': '{{ name | default('grouped') }}',
{% if name == 'grouped' %}
    'group_size': {{ group_size | default(2) }},
{% if npair %}
    'npair': {{ npair }},
{% endif %}
{% if negative_class_mining %}
    'negative_class_mining': {{ negative_class_mining }},
{% endif %}
{% elif name == 'pair' %}
    'positive_ratio': {{ positive_ratio | default(0.5) }},
{% endif %}
    'batch_size': {{ batch_size | default(32) }},
    'num_batches': 100,
    'combine_batches': {{ combine_batches | default(1) }}
}
