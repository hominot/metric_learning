{% if name == 'contrastive' %}
{
    'name': '{{ name }}',
    'alpha': {{ alpha | default(16.0) }},
    'parametrization': 'euclidean_distance',
}
{% elif name == 'triplet' %}
{
    'name': '{{ name }}',
    'alpha': {{ alpha | default(4.0) }},
    'parametrization': 'euclidean_distance',
}
{% elif name == 'margin' %}
{
    'name': '{{ name }}',
    'alpha': {{ alpha | default(0.2) }},
    'beta': {{ beta | default(1.2) }},
    'beta_class': {{ beta_class | default(0.0) }},
    'beta_image': {{ beta_image | default(0.0) }},
    'nu': {{ nu | default(0.0) }},
    'parametrization': 'euclidean_distance',
}
{% elif name == 'latent_position' %}
{
    'name': '{{ name }}',
    'parametrization': '{{ parametrization | default('euclidean_distance') }}',
    {% if npair %}
    'npair': {
        'n': {{ n | default(16) }},
    },
    {% endif %}
    'alpha': {{ alpha | default(16.0) }},
}
{% elif name == 'npair' %}
{
    'name': '{{ name }}',
    'parametrization': '{{ parametrization | default('euclidean_distance') }}',
    'n': {{ n | default(16) }},
    {% if parametrization == 'dot_product' %}
    'lambda': {{ lambda | default(0.01) }},
    {% endif %}
}
{% endif %}
